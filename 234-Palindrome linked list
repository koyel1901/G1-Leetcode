class Solution {
public:
    bool isPalindrome(ListNode* head) {
        vector<int>result;
        ListNode*p1=head;
        while (p1!= NULL) 
        {
            result.push_back(p1->val);
            p1=p1->next;
        }
        int left=0,right=result.size()-1;
        while(left<right)
        {
            if(result[left]!=result[right])
            {
                return false;
            }
            left++;
            right--;
        }
        return true;
    }
};
