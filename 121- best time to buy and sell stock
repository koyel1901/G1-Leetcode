class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int n=prices.size();
        int minprice=prices[0],res=0;
        for(int i=1;i<n;i++)
        {
            minprice=min(minprice,prices[i]);
            res=max(res,prices[i]-minprice);
        }
        return res;
    }
};
