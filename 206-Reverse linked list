class Solution {
public:
    ListNode* reverseList(ListNode* head) {
        if(head==NULL)
        {
            return NULL;
        }
        else
        {
            ListNode*p1=NULL,*p2=head,*p3=NULL;
            while(p2!=NULL)
            {
                p3=p2->next;
                p2->next=p1;
                p1=p2;
                p2=p3;
            }
            head=p1;
        }
        return head;
    }
};
